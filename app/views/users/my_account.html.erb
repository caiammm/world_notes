<li class="body-section">

<!-- TÍTULO -->
	<div align="center"><div class="titulo">Minha Conta</div></div>
	<hr class="titulo_linha">


<!-- MINHA CONTA -->
	<div class="div_tabela">
  	<table class="tablesaw" data-tablesaw-mode="columntoggle" data-tablesaw-minimap>
			<thead>
				<tr>
					<th scope="col" data-tablesaw-sortable-col data-tablesaw-priority="persist">Minhas Publicações</th>
					<th scope="col" data-tablesaw-sortable-col data-tablesaw-priority="1">Data</th>
					<th scope="col" data-tablesaw-sortable-col data-tablesaw-priority="2">Hora</th>
				</tr>
			</thead>
			<tbody>
				<% %w(a b c d e f).each do |post| %>
					<tr>
						<td class="title"><a href=""><%= post %></a></td>
						<td>06 Set 2015</td>
						<td>13:56</td>
					</tr>
				<% end %>
			</tbody>
		</table>
		<p><%= link_to "Sair", logout_path, method: "delete" %></p>
		<p><%= link_to 'Excluir Conta', current_user, method: :delete, data: { confirm: 'Tem certeza?' } %></p>
  </div>
</li> <!-- body-section -->



